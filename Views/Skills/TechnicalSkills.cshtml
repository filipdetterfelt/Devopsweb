@{
    ViewData["Title"] = "TechnicalSkills";
}

@model List<Devopsweb.Models.SkillsModel>



<div class="flex-container">
    <div class="technical-skills">
        <h1 class="group-title">Technical Skills</h1>
        <div class="card-group">
            <div class="card-container">
                <div class="card-deck">
                    @foreach (var skill in Model)
                    {
                            <div class="card">                  
                                <div class="card-body">
                                    <h5 class="card-title">@skill.Title</h5>
                                </div>
                            </div>
                    }
                </div>
            </div>
        </div>
    </div>

    <div class="projects">
        <h1 class="group-title">My Projects</h1>
        <div class="card-group">
            <div class="card-container">
                <div class="card-deck">
                    <div class="card"> 
                        <a href="https://github.com/filipdetterfelt/Backend-Pensionat" target="_blank" style="text-decoration: none; color: inherit;">
                            <div class="card-body">
                                <h5 class="card-title">Pensionat Koriander</h5>
                            </div>
                        </a>
                    </div>
                    <div class="card"> 
                        <a href="https://github.com/filipdetterfelt/Webbshopenv2" target="_blank" style="text-decoration: none; color: inherit;">
                            <div class="card-body">
                                <h5 class="card-title">Webbshopen</h5>
                            </div>
                        </a>
                    </div>
                    <div class="card"> 
                        <a href="https://github.com/filipdetterfelt/The-Exchanger" target="_blank" style="text-decoration: none; color: inherit;">
                            <div class="card-body">
                                <h5 class="card-title">The-Exchanger</h5>
                            </div>
                        </a>
                    </div>
                    <div class="card"> 
                        <a href="https://github.com/filipdetterfelt/Quizkampen" target="_blank" style="text-decoration: none; color: inherit;">
                            <div class="card-body">
                                <h5 class="card-title">Quizkampen</h5>
